#Email
([\s\|,;’]+[a-z0-9\-\._]+@[a-z0-9\-\.]+\.[a-z]{2,4})[\s\|,;:’]+
#Password
^\s*[pass|word|phrase|wd|code]*\s*[:|=|is|was](\s*[a-z0-9\-_\!]+$)
#User:Password 
[ht|f]tp[s]*:\/\/(\w+\:.*)\@\w*\.\w*
#Secret Variable
\s*[a-z0-9\-_]*[S|s]ecret[k|K]ey+\s*[:=]+\s*["'](\S+)["']
#Ip address
(\b(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\b)
#Phone numbers
(^\(?\+?[0-9]*\)??[0-9_\- \(\)]*$)
#Windows Path
(^([a-zA-Z]:|\\{2}\w+)\$?\\\w[\w ]*.*$)


